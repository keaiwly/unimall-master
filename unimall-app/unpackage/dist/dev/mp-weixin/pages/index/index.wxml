<view class="container"><view class="mp-search-box"><input class="ser-input" type="text" value="输入关键字搜索" disabled data-event-opts="{{[['tap',[['naviageToPage',['/pages/product/search']]]]]}}" bindtap="__e"/></view><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" autoplay="true" interval="3000" duration="500" circular data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['naviageToPage',['$0'],[[['carouselList','',index,'url']]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.imgUrl}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="cate-section"><block wx:for="{{categoryButtomList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['naviageToPage',['$0'],[[['categoryButtomList','',index,'url']]]]]]]}}" class="cate-item" bindtap="__e"><image src="{{item.imgUrl}}"></image><text>{{item.title}}</text></view></block></view><block wx:if="{{banner}}"><view data-event-opts="{{[['tap',[['naviageToPage',['$0'],['banner.url']]]]]}}" class="ad-1" bindtap="__e"><image src="{{banner.imgUrl}}" mode="scaleToFill"></image></view></block><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">橱窗推荐</text><text class="tit2">Shop Window</text></view></view><view class="group-section"><swiper class="g-swiper" duration="{{500}}"><block wx:for="{{windowSpuList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index%2===0}}"><swiper-item class="g-swiper-item"><view data-event-opts="{{[['tap',[['navToWindowSuggestSpu',[index]]]]]}}" class="g-item left" bindtap="__e"><image src="{{item.spuImg+'?x-oss-process=style/400px'}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{item.spuTitle}}</text><view class="price-box"><text class="price">{{"￥"+(isVip?item.spuVipPrice/100.0+' [VIP]':item.spuPrice/100.0)}}</text><block wx:if="{{item.spuOriginalPrice>(isVip?item.spuVipPrice:item.spuPrice)}}"><text class="m-price">{{"￥"+item.spuOriginalPrice/100}}</text></block></view><view class="pro-box"><text>{{"累计销售:"+item.spuSales+"件"}}</text></view></view></view><block wx:if="{{index+1<windowSpuList.length}}"><view data-event-opts="{{[['tap',[['navToWindowSuggestSpu',[index+1]]]]]}}" class="g-item right" bindtap="__e"><image src="{{windowSpuList[index+1].spuImg}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{windowSpuList[index+1].spuTitle}}</text><view class="price-box"><text class="price">{{"￥"+(isVip?windowSpuList[index+1].spuVipPrice/100.0+' [VIP]':windowSpuList[index+1].spuPrice/100.0)}}</text><block wx:if="{{windowSpuList[index+1].spuOriginalPrice>(isVip?windowSpuList[index+1].spuVipPrice:windowSpuList[index+1].spuPrice)}}"><text class="m-price">{{"￥"+windowSpuList[index+1].spuOriginalPrice/100.0}}</text></block></view><view class="pro-box"><text>{{"累计销售:"+item.spuSales+"件"}}</text></view></view></view></block><block wx:if="{{index+1===windowSpuList.length}}"><view class="g-item right"></view></block></swiper-item></block></block></swiper></view><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">分类精选</text><text class="tit2">Competitive Products For You</text></view></view><block wx:for="{{categoryPickList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="hot-floor"><view class="floor-img-box"><image class="floor-img" src="{{item.imgUrl}}" mode="scaleToFill"></image></view><scroll-view class="floor-list" scroll-x><view class="scoll-wrapper"><block wx:for="{{item.data}}" wx:for-item="spuItem" wx:for-index="skuIndex" wx:key="skuIndex"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['categoryPickList','',index],['data','',skuIndex,'id']]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{spuItem.img+'?x-oss-process=style/200px'}}" mode="aspectFill"></image><text class="title clamp">{{spuItem.title}}</text><text class="price">{{"￥"+(isVip?spuItem.vipPrice:spuItem.price)/100}}</text></view></block><view data-event-opts="{{[['tap',[['naviageToPage',['$0'],[[['categoryPickList','',index,'url']]]]]]]}}" class="more" bindtap="__e"><text>查看全部</text><text>More+</text></view></view></scroll-view></view></block><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">热销宝贝</text><text class="tit2">Hot Sale</text></view></view><view class="guess-section"><block wx:for="{{salesTop}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['salesTop','',index,'id']]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.img+'?x-oss-process=style/400px'}}" mode="aspectFill"></image></view><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+(isVip?item.vipPrice/100+' [VIP]':item.price/100)}}</text></view></block></view></view>